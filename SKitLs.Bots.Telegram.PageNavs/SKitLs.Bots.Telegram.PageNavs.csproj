<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>$(AssemblyName)</Title>
    <Version>1.3.1</Version>
    <Authors>SKitLs, Sargeras02</Authors>
    <Copyright>Copyright (C) Sargeras02 2023</Copyright>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <FileVersion>$(AssemblyVersion)</FileVersion>
    <AssemblyVersion>1.3.1</AssemblyVersion>
    <PackageReleaseNotes>Summaries and naming updates.</PackageReleaseNotes>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageIcon>libIcon_PN.png</PackageIcon>
    <Description>Extension for SKitLs.Bots.Telegram.Core. Pay attention to GitHub.

Allows to create custom menus, using inline keyboards and callbacks.</Description>
    <PackageProjectUrl>https://github.com/Sargeras02/SKitLs.Bots.Telegram.git</PackageProjectUrl>
    <PackageTags>Telegram, Bots, Management</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Exceptions\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\libIcon_PN.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SKitLs.Bots.Telegram.AdvancedMessages" Version="1.0.0" />
    <PackageReference Include="SKitLs.Bots.Telegram.ArgedInteractions" Version="1.2.1" />
    <PackageReference Include="SKitLs.Bots.Telegram.Core" Version="1.4.3" />
    <PackageReference Include="Telegram.Bot" Version="19.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Update="LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
